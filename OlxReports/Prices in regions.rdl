  <DataSets>
    <DataSet Name="AveragePriceM2">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Region">
            <Value>=Parameters!Region.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>with PricePerM2(LocationKey, PriceM2) as (
	select LocationKey,
Price / Area AS PriceM2
	from FactOfferSnapshot
) 
select l.City, AVG(ppm.PriceM2) as AvgPriceM2, count(*) offers
from PricePerM2 ppm
join DimLocation l on ppm.LocationKey = l.LocationKey
where l.Region in (@Region)
group by l.City
having AVG(ppm.PriceM2) is not null
order by 2</CommandText>
      </Query>
      <Fields>
        <Field Name="City">
          <DataField>City</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="AvgPriceM2">
          <DataField>AvgPriceM2</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="offers">
          <DataField>offers</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
    <DataSet Name="Regions">
